{
  "name": "contributte/monolog",
  "description": "Monolog integration into Nette Framework",
  "keywords": [
    "nette",
    "monolog",
    "logging"
  ],
  "type": "library",
  "license": "MPL-2.0",
  "homepage": "https://github.com/contributte/monolog",
  "authors": [
    {
      "name": "Marek Barto≈°",
      "homepage": "https://marek-bartos.cz"
    }
  ],
  "require": {
    "php": "^7.2.0",
    "contributte/di": "^0.4.0",
    "monolog/monolog": "^2.0.0",
    "nette/utils": "~3.0.0 || ~3.1.0"
  },
  "require-dev": {
    "ninjify/qa": "^0.10.0",
    "phpstan/phpstan": "^0.12",
    "phpstan/extension-installer": "^1.0",
    "phpstan/phpstan-deprecation-rules": "^0.12",
    "phpstan/phpstan-nette": "^0.12",
    "phpstan/phpstan-strict-rules": "^0.12",
    "phpunit/phpunit": "^8.1.3",
    "tracy/tracy": "~2.6.2 || ~2.7.0"
  },
  "conflict": {
    "tracy/tracy": "<2.6.2"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Monolog\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Contributte\\Monolog\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.5.x-dev"
    }
  }
}
