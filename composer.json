{
  "name": "contributte/monolog",
  "description": "Monolog integration into Nette Framework",
  "keywords": [
    "nette",
    "monolog",
    "logging"
  ],
  "type": "library",
  "license": "MPL-2.0",
  "homepage": "https://github.com/contributte/monolog",
  "authors": [
    {
      "name": "Marek BartoÅ¡",
      "homepage": "https://marek-bartos.cz"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "monolog/monolog": "^1.23.0",
    "nette/di": "~2.4.12",
    "nette/utils": "~2.5.2"
  },
  "require-dev": {
    "ninjify/qa": "^0.9.0",
    "phpstan/phpstan-deprecation-rules": "^0.11",
    "phpstan/phpstan-nette": "^0.11",
    "phpstan/phpstan-shim": "^0.11",
    "phpstan/phpstan-strict-rules": "^0.11",
    "phpunit/phpunit": "^8.1.3",
    "tracy/tracy": "~2.5.0"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Monolog\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Contributte\\Monolog\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.3.x-dev"
    }
  }
}
